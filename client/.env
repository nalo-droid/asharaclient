# API URL
REACT_APP_API_URL=http://localhost:5000/api

# Authentication
REACT_APP_JWT_STORAGE_KEY=arch_design_token
REACT_APP_USER_STORAGE_KEY=arch_design_user

# File Upload
REACT_APP_MAX_FILE_SIZE=5242880 # 5MB in bytes
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg

# Design Categories
REACT_APP_DESIGN_CATEGORIES=Residential,Commercial,Industrial

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_CHAT=false

# Social Media Links (if needed)
REACT_APP_FACEBOOK_URL=https://facebook.com/your-page
REACT_APP_INSTAGRAM_URL=https://instagram.com/your-page
REACT_APP_LINKEDIN_URL=https://linkedin.com/your-page

# Contact Information
REACT_APP_CONTACT_EMAIL=contact@yourcompany.com
REACT_APP_CONTACT_PHONE=+1234567890

# Google Maps API Key (if using maps)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
